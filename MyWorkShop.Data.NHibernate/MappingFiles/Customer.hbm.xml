<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
            assembly="MyWorkShop.Model"
            namespace="MyWorkShop.Model.Entities">
  <class name="Customer" table="MyWorkShop_Customer">
    <id name="Id" type="Int32">
      <generator class="native"/>
    </id>
    <property name="Name" type="String" length="50" not-null="true"/>
    <property name="Address" type="String" length="100"/>
    <property name="Phone" type="String" length="50"/>
  </class>
</hibernate-mapping>